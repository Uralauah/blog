      "code": {
        "caption": [],
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "public void calc(int r, int c, int sum){\n        if(r==row){\n            if(c==col){\n                if(check())\n                    ans = Math.min(ans, sum);\n                return;    \n            }\n            \n            calc(r, c+1, sum);\n            \n            flip(c,1);\n            calc(r,c+1, sum+1);\n            flip(c,1);\n            \n            return;\n        }\n        \n        calc(r+1, c, sum);\n            \n        flip(r,0);\n        calc(r+1,c,sum+1);\n        flip(r,0);\n    }",
              "link": null
            },
            "annotations": {
              "bold": false,
              "italic": false,
              "strikethrough": false,
              "underline": false,
              "code": false,
              "color": "default"
            },
            "plain_text": "public void calc(int r, int c, int sum){\n        if(r==row){\n            if(c==col){\n                if(check())\n                    ans = Math.min(ans, sum);\n                return;    \n            }\n            \n            calc(r, c+1, sum);\n            \n            flip(c,1);\n            calc(r,c+1, sum+1);\n            flip(c,1);\n            \n            return;\n        }\n        \n        calc(r+1, c, sum);\n            \n        flip(r,0);\n        calc(r+1,c,sum+1);\n        flip(r,0);\n    }",
            "href": null
          }
        ],
        "language": "java"
