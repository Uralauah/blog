            "plain_text": "#include <iostream>\n#include <vector>\n\nusing namespace std;\n\nint dp[101][100001] = {0};\n\nint main() {\n    int n, k, w, v, sum;\n    cin >> n >> k;\n    vector<pair<int, int>> vec;\n    for (int i = 1; i <= n; i++) {\n        cin >> w >> v;\n        vec.push_back({w, v});\n    \n\n    for (int i = 1; i <= n; i++) {\n        for (int j = 1; j <= k; j++) {\n            if (vec[i - 1].first > j) {\n                dp[i][j] = dp[i - 1][j];\n            } else {\n                dp[i][j] = max(dp[i - 1][j], dp[i - 1][j - vec[i - 1].first] + vec[i - 1].second);\n            }\n        }\n    }\n    cout << dp[n][k];\n}",
