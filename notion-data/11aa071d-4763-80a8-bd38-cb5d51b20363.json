        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "for (int i = 1; i <= n; i++) {\n            for (int j = i; j <= n; j++) {\n                for (int l = 2; l <= m; l++) {\n                    if (dp[i][j][l-1] != 0) {\n                        for (int o = l; o <= n; o++) {\n                            dp[i][o][l] = Math.max(dp[i][o][l], dp[i][j][l-1] + dp[j][o][l-1]);\n                        }\n                    }\n                }\n            }\n        }",
              "link": null
            },
            "annotations": {
              "bold": false,
              "italic": false,
              "strikethrough": false,
              "underline": false,
              "code": false,
              "color": "default"
            },
            "plain_text": "for (int i = 1; i <= n; i++) {\n            for (int j = i; j <= n; j++) {\n                for (int l = 2; l <= m; l++) {\n                    if (dp[i][j][l-1] != 0) {\n                        for (int o = l; o <= n; o++) {\n                            dp[i][o][l] = Math.max(dp[i][o][l], dp[i][j][l-1] + dp[j][o][l-1]);\n                        }\n                    }\n                }\n            }\n        }",
            "href": null
          }
        ],
